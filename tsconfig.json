    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "resolveJsonModule": true,
    "incremental": true,
    "typeRoots": [
      "node_modules/@types",
      "node_modules/discord.js/typings"
    ],
  },
  "lib": ["es2015"]
}
